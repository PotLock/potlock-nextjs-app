{
  "editor.formatOnSave": true,
  "eslint.validate": [
    "typescript"
  ],
  "editor.rulers": [
    100
  ],
  "editor.tabSize": 2,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll": "explicit"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "errorLens.followCursor": "allLinesExceptActive",
  "errorLens.scrollbarHackEnabled": true,
  "errorLens.margin": "1%",
  "errorLens.fontSize": "19px",
  "errorLens.fontStyleItalic": true,
  "errorLens.gutterIconSet": "borderless",
  "errorLens.replaceLinebreaksSymbol": "\t",
  "errorLens.statusBarMessageEnabled": true,
  "errorLens.statusBarCommand": "goToProblem",
  "errorLens.statusBarColorsEnabled": true,
  "errorLens.statusBarMessageType": "activeLine",
  "errorLens.warningGutterIconColor": "#ff942f",
  "cSpell.words": [
    "Attributify",
    "bitget",
    "builddao",
    "camelcase",
    "campaignstest",
    "cmdk",
    "codegen",
    "colocation",
    "cooldown",
    "data-testid",
    "datetime",
    "defi",
    "dontcare",
    "fastnear",
    "foodbank",
    "formkit",
    "graylisted",
    "hookform",
    "kubb",
    "linktree",
    "METAPOOL",
    "mpdao",
    "mpdaovoting",
    "NADABOT",
    "narwallets",
    "naxios",
    "nearfi",
    "openapi",
    "overscan",
    "potfactory",
    "POTLOCK",
    "precommit",
    "ramper",
    "retropgf",
    "RPGF",
    "sessionid",
    "shadcn",
    "socialdb",
    "SOURCECODE",
    "stnear",
    "svgr",
    "typecheck",
    "uidotdev",
    "unocss",
    "upvote",
    "Upvoted",
    "usehooks",
